! Black color that will not be affected by bold highlighting.
*.color66: #120018
*color66:  #120018

! Xclock colors.
XClock*foreground: #d097c8
XClock*background: #120018
XClock*majorColor:  rgba:d0/97/c8/ff
XClock*minorColor:  rgba:d0/97/c8/ff
XClock*hourColor:   rgba:d0/97/c8/ff
XClock*minuteColor: rgba:d0/97/c8/ff
XClock*secondColor: rgba:d0/97/c8/ff

! Set depth to make transparency work.
URxvt*depth: 32
