Xcursor.size: 12

XTerm*faceName: Hack
XTerm*faceSize: 16

xterm*background: #1e1e1e
xterm*foreground: #ffffff

xterm*savelines: 16384
xterm*ScrollBar: false

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault


! X colors.
! Generated by 'wal'
*foreground:        #d097c8
*background:        #120018
*.foreground:       #d097c8
*.background:       #120018
emacs*foreground:   #d097c8
emacs*background:   #120018
URxvt*foreground:   #d097c8
XTerm*foreground:   #d097c8
UXTerm*foreground:  #d097c8
URxvt*background:   [100]#120018
XTerm*background:   #120018
UXTerm*background:  #120018
URxvt*cursorColor:  #d097c8
XTerm*cursorColor:  #d097c8
UXTerm*cursorColor: #d097c8
URxvt*borderColor:  [100]#120018

